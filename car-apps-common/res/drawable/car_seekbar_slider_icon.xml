<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
            android:autoMirrored="true">
    <item android:id="@+id/seekbar_progress_icon"
          android:gravity="center"
          android:height="@dimen/car_seekbar_icon_size"
          android:width="@dimen/car_seekbar_icon_size">
        <inset
            android:insetTop="@dimen/car_rotary_highlight_ring_thickness"
            android:insetBottom="@dimen/car_rotary_highlight_ring_thickness"
            android:insetLeft="@dimen/car_rotary_highlight_ring_thickness"
            android:insetRight="@dimen/car_rotary_highlight_ring_thickness"
            android:drawable="@drawable/car_seekbar_default_icon">
        </inset>
    </item>
    <item
        android:id="@+id/seekbar_progress_icon_focus"
        android:drawable="@drawable/car_seekbar_icon_selector">
    </item>

</layer-list>

